<div class="row">
  <%= render partial: "strails/backend/product_navigation", locals: { product: @product } %>
  <div class="product">
    <div class="card">
      <div class="card-header">
        <h3><%= @product.name %></h3>
      </div>
      <div class="card-body">
        <%= render 'form', product: @product %>
      </div>
    </div>
  </div>
</div>
